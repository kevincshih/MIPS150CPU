library verilog;
use verilog.vl_types.all;
entity bios_mem is
    port(
        clka            : in     vl_logic;
        ena             : in     vl_logic;
        addra           : in     vl_logic_vector(11 downto 0);
        douta           : out    vl_logic_vector(31 downto 0);
        clkb            : in     vl_logic;
        enb             : in     vl_logic;
        addrb           : in     vl_logic_vector(11 downto 0);
        doutb           : out    vl_logic_vector(31 downto 0)
    );
end bios_mem;
